                            <script>
                                let withdraw_success = document.querySelector('.withdraw_success')
                                let withdraw_failed = document.querySelector('.withdraw_failed')
                                let withdraw_error = document.querySelector('#withdraw_error')
                                let withdraw_btn = document.querySelector('#withdraw_btn')
                                let amount = document.querySelector('#amount1')
                                let password = document.querySelector('#password1')
                                let balance = parseInt("<%= user1.balance %>")
                                let password11 = "<%= user.password %>"
                                let form = document.querySelector('#withdraw_form')
                                // console.log(password11)

                                form.addEventListener('submit', (e)=>{
                                    e.preventDefault()
                                    // console.log(amount)

                                    if (amount.value > balance ){
                                        withdraw_failed.classList.remove("hidden")
                                        withdraw_error.innerHTML = "Insufficient Balance, Please Try Again"
                                    } else if (password.value != password11){
                                        // console.log(password.value)
                                        withdraw_failed.classList.remove("hidden")
                                        withdraw_error.innerHTML = "Incorrect Password, Please Try Again"
                                    } else{
                                        withdraw_btn.innerHTML="Loading...."
                                        const formdata = new FormData(form);
                                        const payload = new URLSearchParams(formdata);
                                        fetch('/withdraw', {
                                            method:'POST',
                                            body: payload,
                                            // redirect: "follow"
                                        }).then(res =>{
                                            if (res.ok){
                                                withdraw_success.classList.remove("hidden")
                                                withdraw_btn.innerHTML="Successful!"
                                                // res.json()
                                            }
                                        }).catch(err =>{
                                            console.log(err)
                                            withdraw_failed.classList.remove("hidden")
                                            withdraw_error.innerHTML = "Withdrawal Request Failed, Please Try Again"
                                        })
                                    }
                                })

