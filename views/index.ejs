                        </table>
                
                    </div>
                </div>
                <!-- <div class="w-full h-[200px] overflow-scroll" > -->
                    <!-- each bot history -->
                <div id="eachHistory" class="my-[10px]">
                    <div class="flex justify-center items-center">
                        <div class="botContain ">
                            <% if (botTxns.length > 0) { %>
                            <% botTxns.reverse().forEach(botTxn =>{ %>
                                <table class="w-full mx-auto my-2">
                                    <tr>
                                        <th class=" text-left"> <span class="font-semibold text-sm text-gray-800">BTCUSD <span class="text-orange-500"><%= botTxn.type  %></span> </span> </th>
                                         <td id="amount" class="text-right font-bold text-sm tracking-wide">
                                            <% if (botTxn.loss){ %>
                                                <span class="text-red-600">-$<%= botTxn.amount%></span>
                                            <% }else{ %>
                                                <span class="text-green-600">$<%= botTxn.amount%></span>
                                            <% } %>
                                        </td>
                                    </tr>
                                    <tr>
                                        <th id="btcPrice" class=" text-left font-light text-gray-500 text-xs"><%= botTxn.btcPrice %>.00</th>
